#!/usr/bin/python
import sys

data = {}

line = sys.stdin.readline()
while line:
        if line[0] != "@":

                record = line.split()
                data.setdefault(record[2], 0)
                data[record[2]] += 1
		
        line = sys.stdin.readline()

READS = [int(value) for value in data.values()]
TOTAL = sum(READS)

#PRINT OUTPUT

for key, value in data.items():
        print key, "%.2f%%" %(int(value)/TOTAL*100)


